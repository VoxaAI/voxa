    "echo",
    "amazon"
  ],
  "author": "Rain Agency <npm@rain.agency> (http://rain.agency)",
  "engines": {
    "node": ">=4.3.2"
  },
  "license": "MIT",
  "dependencies": {
    "alexa-skill-test-framework": "^1.1.2",
    "bluebird": "^3.5.1",
    "capitalize": "^1.0.0",
    "debug": "^3.1.0",
    "i18next": "^9.0.1",
    "lodash": "^4.13.1",
    "striptags": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
